{
    "name": "biologer/biologer",
    "type": "project",
    "description": "Database of wildlife of Serbia",
    "keywords": ["biologer", "reptile", "amphibia", "butterfly", "insect", "serbia"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nenad Živanović",
            "email": "nen.zivanovic@gmail.com",
            "homepage": "https://nenadzivanovic.in.rs",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "box/spout": "^2.7",
        "dimsav/laravel-translatable": "^9.0",
        "doctrine/dbal": "^2.8",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.4",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.0",
        "laravel/tinker": "^1.0",
        "maxakawizard/json-collection-parser": "^1.1",
        "mcamara/laravel-localization": "^1.3.18",
        "meyfa/php-svg": "^0.9.0",
        "myclabs/php-enum": "^1.6",
        "nikazooz/laravel-captcha": "^0.1.0",
        "php-coord/php-coord": "^2.1",
        "runalyze/dem-reader": "^1.1",
        "sofa/eloquence-mappable": "^5.6",
        "spatie/laravel-activitylog": "^3.2",
        "spatie/laravel-backup": "^6.1",
        "spatie/laravel-cookie-consent": "^2.2",
        "spatie/laravel-menu": "^3.1",
        "stevebauman/purify": "^2.0",
        "tightenco/ziggy": "^0.6"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "itsgoingd/clockwork": "^3.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.5",
        "wnx/laravel-stats": "^1.3"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": "phpunit"
    }
}
