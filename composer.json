{
    "name": "biologer/biologer",
    "type": "project",
    "description": "Database of wildlife of Serbia",
    "keywords": ["biologer", "reptile", "amphibia", "butterfly", "insect", "serbia"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nenad Živanović",
            "email": "nen.zivanovic@gmail.com",
            "homepage": "https://nenadzivanovic.in.rs",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "box/spout": "^2.7",
        "astrotomic/laravel-translatable": "^11.5",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.5",
        "laravel/framework": "^6.4",
        "laravel/passport": "^7.0",
        "laravel/tinker": "^1.0.6",
        "maxakawizard/json-collection-parser": "^1.5",
        "mcamara/laravel-localization": "^1.4.6",
        "meyfa/php-svg": "^0.9.1",
        "myclabs/php-enum": "^1.7.2",
        "nikazooz/laravel-captcha": "^1.0",
        "php-coord/php-coord": "^3.1",
        "runalyze/dem-reader": "^1.1",
        "spatie/laravel-activitylog": "^3.8",
        "spatie/laravel-backup": "^6.4",
        "spatie/laravel-cookie-consent": "^2.9",
        "spatie/laravel-menu": "^3.4",
        "stevebauman/purify": "^3.0",
        "tightenco/ziggy": "^0.8"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "itsgoingd/clockwork": "^4.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "wnx/laravel-stats": "^2.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": "phpunit"
    }
}
