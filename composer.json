{
    "name": "nikazooz/biologer",
    "description": "Database of wildlife in Serbia",
    "keywords": ["biologer", "reptile", "amphibia", "insect", "serbia"],
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "czim/laravel-localization-route-cache": "^0.9.6",
        "dimsav/laravel-translatable": "^8.1",
        "fideloper/proxy": "~3.3",
        "laravel/framework": "5.5.*",
        "laravel/passport": "^4.0",
        "laravel/tinker": "~1.0",
        "mcamara/laravel-localization": "^1.3",
        "nikazooz/laravel-captcha": "^0.1.0",
        "php-coord/php-coord": "^2.1",
        "sofa/eloquence-mappable": "^5.5",
        "spatie/laravel-activitylog": "^2.5",
        "tightenco/ziggy": "^0.4.1"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "itsgoingd/clockwork": "^2.0",
        "mockery/mockery": "^1.0.0",
        "phpunit/phpunit": "~6.0",
        "wnx/laravel-stats": "^1.3.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Support/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [

            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "test": "phpunit"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
