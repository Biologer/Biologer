{
    "name": "biologer/biologer",
    "type": "project",
    "description": "Database of wildlife of Serbia",
    "keywords": ["biologer", "reptile", "amphibia", "butterfly", "insect", "serbia"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nenad Živanović",
            "email": "nen.zivanovic@gmail.com",
            "homepage": "https://nenadzivanovic.in.rs",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "astrotomic/laravel-translatable": "^11.9.1",
        "box/spout": "^2.7.3",
        "doctrine/dbal": "^3.10.3",
        "google/auth": "^1.37.2",
        "guzzlehttp/guzzle": "^7.10.0",
        "intervention/image": "^2.7.2",
        "kreait/firebase-php": "^6.9.6",
        "laravel/framework": "^v10.49.1",
        "laravel/passport": "^v11.10.6",
        "laravel/tinker": "^2.10.1",
        "laravel/ui": "^v4.3.0",
        "league/flysystem-aws-s3-v3": "^3.8.0",
        "maxakawizard/json-collection-parser": "^1.10.0",
        "mcamara/laravel-localization": "^1.8.0",
        "meyfa/php-svg": "^0.11.3",
        "myclabs/php-enum": "^1.8.5",
        "nikazooz/laravel-captcha": "^v1.6.0",
        "php-coord/php-coord": "^4.7.1",
        "runalyze/dem-reader": "^1.1.0",
        "spatie/laravel-activitylog": "^4.10.2",
        "spatie/laravel-backup": "^8.0",
        "spatie/laravel-cookie-consent": "^3.0.0",
        "spatie/laravel-menu": "^4.2.1",
        "stevebauman/purify": "^v5.1.1",
        "tightenco/ziggy": "^0.9.4"
    },
    "require-dev": {
        "brianium/paratest": "^v7.0.0",
        "spatie/laravel-ignition": "^2.9.1",
        "fakerphp/faker": "^1.24.1",
        "friendsofphp/php-cs-fixer": "^3.89.2",
        "itsgoingd/clockwork": "^5.3.5",
        "laravel/sail": "^1.19.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^v7.12.0",
        "phpunit/phpunit": "^10.5.58",
        "wnx/laravel-stats": "^2.10.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.2"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": "phpunit"
    }
}
