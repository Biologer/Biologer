{
    "name": "biologer/biologer",
    "type": "project",
    "description": "Database of wildlife of Serbia",
    "keywords": ["biologer", "reptile", "amphibia", "butterfly", "insect", "serbia"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nenad Živanović",
            "email": "nen.zivanovic@gmail.com",
            "homepage": "https://nenadzivanovic.in.rs",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "astrotomic/laravel-translatable": "^11.9.1",
        "box/spout": "^2.7",
        "guzzlehttp/guzzle": "^7.3",
        "intervention/image": "^2.6.1",
        "laravel/framework": "^v11.44.1",
        "laravel/passport": "^v12.4.2",
        "laravel/tinker": "^2.6.1",
        "laravel/ui": "^v4.2.2",
        "league/flysystem-aws-s3-v3": "^3.8.0",
        "maxakawizard/json-collection-parser": "^1.8.0",
        "mcamara/laravel-localization": "^v2.3.0",
        "meyfa/php-svg": "^0.11.2",
        "myclabs/php-enum": "^1.8.3",
        "nikazooz/laravel-captcha": "v1.5.0",
        "php-coord/php-coord": "^4.4",
        "runalyze/dem-reader": "^1.1",
        "spatie/laravel-activitylog": "^4.0.0",
        "spatie/laravel-backup": "^8.0.0",
        "spatie/laravel-cookie-consent": "^3.2.4",
        "spatie/laravel-menu": "^4.0.0",
        "stevebauman/purify": "^v6.3.0",
        "tightenco/ziggy": "^v2.5.2"
    },
    "require-dev": {
        "brianium/paratest": "^v7.0.0",
        "spatie/laravel-ignition": "^2.0.0",
        "fakerphp/faker": "^1.15",
        "friendsofphp/php-cs-fixer": "^3.1",
        "itsgoingd/clockwork": "^5.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^v8.1.0",
        "phpunit/phpunit": "^10.0.0",
        "wnx/laravel-stats": "^2.5.2"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.3"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": "phpunit"
    }
}
